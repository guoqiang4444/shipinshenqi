<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomProfileGridView">
        <attr name="cell_width" format="dimension" />
        <attr name="cell_height" format="dimension" />
        <attr name="horizontal_divider_space" format="dimension"></attr>
        <attr name="vertical_divider_space" format="dimension" />
        <attr name="column_num" format="integer" />
    </declare-styleable>

    <!-- Defines the custom XML attributes supported for a HorizontalListView -->
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>
    <declare-styleable name="ActionBar">
        <attr name="title" format="string" localization="suggested" />
    </declare-styleable>

</resources>